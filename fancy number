#include<stdio.h>
#include<conio.h>
int main()
{
	int n;
	int r,ecount=0,ocount=0;
	scanf("%d",&n);
	while(n>0)
	{
		r=n%10;
		if(r%2==0)
		ecount=ecount+1;
		else
		ocount=ocount+1;
		n=n/10;
    if(ecount!=0)
    {
       printf("Fancy Number\n",ecount);
    }
    else
    {
        printf("Not Fancy Number\n",ocount);
	}
	return 0;
}
